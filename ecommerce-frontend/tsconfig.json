{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "esnext",
    "target": "es5",
    "strict": true,
    "moduleResolution": "node",
    "ignoreDeprecations": "6.0",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src", "jest.config.ts"]
}
